  <groups>
    <group name="ReverseBuy">
      <displayName>ReverseBuy</displayName>
    </group>
    <group name="Clients" parentPath="/ReverseBuy">
      <displayName>Clients</displayName>
      <description>This group represents registered buyers who purchase products or services from agents. They can browse listings, place orders, and interact with agents.</description>
    </group>
    <group name="Agents" parentPath="/ReverseBuy">
      <displayName>Agents</displayName>
      <description>This group consists of users who list and sell products or services. They manage listings, set prices, and handle transactions with clients.</description>
    </group>
    <group name="Administration" parentPath="/ReverseBuy">
      <displayName>Administration</displayName>
      <description>Oversees the auction marketplace, managing agents, clients, and overall system configuration.</description>
    </group>
    <group name="Unregistered Users" parentPath="/ReverseBuy">
      <displayName>Unregistered Users</displayName>
      <description>Allows visitors to view certain items or service listings without making purchases.
</description>
    </group>
  </groups>
  <memberships>
    <membership>
      <userName>john_admin</userName>
      <roleName>Admin</roleName>
